#lang sicp
(define x (list (list 1 2) (list 3 4)))
x ;; ((1 2) (3 4))
(reverse x) ;; ((3 4) (1 2))
(define (deep-reverse items)
  (define (iter items result)
    (if (null? items)
        result
        (iter (cdr items) 
              (cons (deep-reverse (car items)) result))))
  (if (pair? items)
      (iter items nil)
      items))
(deep-reverse x)
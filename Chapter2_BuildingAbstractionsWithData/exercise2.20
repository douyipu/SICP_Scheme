#lang sicp
(define (same-parity x . y)
  (define (iter f? items result)
    (cond ((null? items) (reverse result))
          ((f? (car items))
           (iter f? (cdr items) (cons (car items) result)))
          (else (iter f? (cdr items) result))))
  (if (odd? x)
      (iter odd? y (list x))
      (iter even? y (list x))))
(same-parity 1 2 3 4 5 6 7)
(same-parity 2 3 4 5 6 7)